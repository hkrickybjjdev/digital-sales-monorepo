meta {
  name: List Page Contents
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/api/v1/pages/:pageId/contents
  body: none
}

vars {
  pageId: example-page-id
}

headers {
  Content-Type: application/json
  Authorization: Bearer {{authToken}}
}

tests {
  test("Status code is 200", function() {
    expect(res.status).to.equal(200);
  });

  test("Response has contents array", function() {
    expect(res.body.contents).to.be.an('array');
  });
}